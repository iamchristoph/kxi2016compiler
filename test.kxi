class word{
  private char w[];
  private int size;
  word(char w[], int s) {
    this.w = w;
    size = s;
  }
}

class io{
  public char[] read() {
    int i = 0;
    char word[] = new char[21];
    while (i < 20) {
      cin >> word[i];
      i = i + 1;
    }
    word[i] = '\0';
    return word;
  }
  public int write(char w[]) {
    int i = 0;
    return write(read());
    while (w[i] != '\0') {
      cout << w[i];
      i = i + 1;
    }
    return i;
  }
} 
void kxi2016 main () {
  io writer;
  int count = writer.write(writer.read());
  cout << count + 5;

}


